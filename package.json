{
  "name": "@warpmetrics/warp",
  "version": "0.0.9",
  "description": "Measure your agents, not your LLM calls.",
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./src/index.d.ts"
    }
  },
  "files": [
    "src",
    "!src/**/*.test.js"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "preversion": "vitest run --coverage",
    "release:patch": "npm version patch && git push origin main --tags",
    "release:minor": "npm version minor && git push origin main --tags"
  },
  "keywords": [
    "ai",
    "agents",
    "llm",
    "observability",
    "openai",
    "anthropic",
    "claude",
    "gpt",
    "tracing",
    "monitoring"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/warpmetrics/warp"
  },
  "dependencies": {
    "ulid": "^3.0.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1",
    "vitest": "^1.2.0"
  }
}
